{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"react-app",
		"react-app/jest",
		"eslint:recommended",
		"plugin:jsx-a11y/recommended"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["react", "jsx-a11y"],
	"rules": {
		"react/prop-types": "off",
		"react/react-in-jsx-scope": "off",
		"jsx-a11y/anchor-is-valid": "warn",
		"no-unused-vars": "warn",
		"no-console": "warn"
	}
}
